{
    "name": "job-opta",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "run-p -l start:*",
        "jest": "npx jest --detectOpenHandles",
        "__start:server": "DEBUG=job* node . --type=process --process=serverHttp",
        "start:gameday": "DEBUG=job* node . --type=process --process=gameDayProcess",
        "start:data": "DEBUG=job* node . --type=process --process=dataProcess",
        "start:listener": "DEBUG=job* node . --type=process --process=listenProcess",
        "start:purgeDataProcess": "DEBUG=job* node . --type=process --process=purgeDataProcess",
        "migration": "DEBUG=job* node main --type=process --process=migrationUp",
        "migration:custom": "DEBUG=job* node main --type=process --process=migrationUpCustom",
        "extract": "run-s -l extract:*",
        "extract:population": "DEBUG=job* node main.js --type=extract --process=populationStats",
        "extract:ot2": "DEBUG=job* node main.js --type=extract --process=tournamentCalendarsOT2",
        "extract:tm1": "DEBUG=job* node main.js --type=extract --process=teamTM1",
        "extract:pe2": "DEBUG=job* node main.js --type=extract --process=playerCareerPE2",
        "extract:ma0": "DEBUG=job* node main.js --type=extract --process=tournamentScheduleMA0",
        "extract:ma1": "DEBUG=job* node main.js --type=extract --process=matchMA1",
        "extract:ma2": "DEBUG=job* node main.js --type=extract --process=matchStatsMA2",
        "extract:ma6": "DEBUG=job* node main.js --type=extract --process=commentaryMA6",
        "extract:tm2": "DEBUG=job* node main.js --type=extract --process=teamStandingsTM2",
        "load": "run-s -l load:*",
        "load:population": "DEBUG=job* node main.js --type=load --process=population",
        "load:team": "DEBUG=job* node main.js --type=load --process=team",
        "load:league": "DEBUG=job* node main.js --type=load --process=league",
        "load:player": "DEBUG=job* node main.js --type=load --process=player",
        "load:playerseason": "DEBUG=job* node main.js --type=load --process=playerSeason",
        "load:playerleague": "DEBUG=job* node main.js --type=load --process=playerLeague",
        "load:event": "DEBUG=job* node main.js --type=load --process=event",
        "load:eventstats": "DEBUG=job* node main.js --type=load --process=eventStats",
        "load:playerstats": "DEBUG=job* node main.js --type=load --process=playerStats",
        "load:lineup": "DEBUG=job* node main.js --type=load --process=gameLineUp",
        "load:subs": "DEBUG=job* node main.js --type=load --process=gameActionSubs",
        "load:card": "DEBUG=job* node main.js --type=load --process=gameActionCard",
        "load:goal": "DEBUG=job* node main.js --type=load --process=gameActionGoal",
        "load:comm": "DEBUG=job* node main.js --type=load --process=gameCommentary",
        "load:stan": "DEBUG=job* node main.js --type=load --process=standing",
        "load:opta": "DEBUG=job* node main.js --type=load --process=optaId",
        "test": "echo \"Error: no test specified\" && exit 1",
        "__build": "npx pkg . -o ./bin/app",
        "deploy": "run-s -l deploy:*",
        "deploy:config": "heroku git:remote -a cruzeiro-job-opta",
        "deploy:build": "git push heroku master:main",
        "cloud:log": "heroku logs --app cruzeiro-job-opta --tail",
        "cloud:start": "heroku ps:scale worker=1 --app cruzeiro-job-opta",
        "cloud:stop": "heroku ps:scale worker=0 --app cruzeiro-job-opta",
        "cloud:sf:start": "heroku ps:scale worker=1 --app cruzeiro-job-sf",
        "cloud:sf:stop": "heroku ps:scale worker=0 --app cruzeiro-job-sf"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "async": "^3.2.0",
        "axios": "^0.19.2",
        "cross-env": "^7.0.3",
        "debug": "^4.1.1",
        "dotenv-safe": "^8.1.0",
        "express": "^4.17.1",
        "job-opta": "file:",
        "massive": "^6.9.1",
        "md5": "^2.3.0",
        "moment-timezone": "^0.5.33",
        "node-cron": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "object-hash": "^2.2.0",
        "pg-boss": "^6.1.0",
        "pg-monitor": "^1.4.1",
        "postgres": "^3.4.1",
        "throng": "^5.0.0",
        "uuid": "^3.3.3",
        "yargs": "^17.0.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "pkg": "^4.5.1"
    },
    "bin": "bin.js",
    "pkg": {
        "scripts": [
            "helpers/**/*.js",
            "lib/**/*.js"
        ],
        "targets": [
            "node12-macos-x64",
            "node12-linux-x64",
            "node12-win-x64"
        ],
        "outputPath": "dist"
    }
}
